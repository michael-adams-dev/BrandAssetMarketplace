<div class="container">
  <% @seller_profiles.each do |profile| %>
    <div class="profile-card">
      <%= link_to seller_profile_path(profile.id) do %>
        <% if profile.brand_logo.attached? %>
          <%= image_tag profile.brand_logo, class: "profile-img" %>
        <% end %>
        <h3 class="profile-title">
          <%= profile.brand_name %>
        </h3>
      <% end %>
    </div>
  <% end %>
</div>